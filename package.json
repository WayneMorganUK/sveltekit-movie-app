		"@sveltejs/kit": "^1.0.0-next.239",
		"@tailwindcss/line-clamp": "^0.3.1",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"autoprefixer": "^10.4.2",
		"eslint": "^8.7.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.4.0",
		"node-fetch": "^3.2.0",
