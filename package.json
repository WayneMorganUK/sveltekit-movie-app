		"@sveltejs/kit": "^1.0.0-next.179",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"autoprefixer": "^10.3.6",
		"cssnano": "^5.0.8",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
